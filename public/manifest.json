{
  "manifest_version": 1,
  "name": "houser tab",
  "version": "1.0.0",
  "description": "程序员适用的 标签页",

  "icons":
  {
      "16": "img/favicon.png",
      "48": "img/favicon.png",
      "128": "img/favicon.png"
  },

  "permissions":
  [
      "webRequest", // web请求
      "webRequestBlocking",
      "storage", // 插件本地存储
  ],

  "homepage_url": "https://houserqu.com",

  "chrome_url_overrides":
  {
      // 覆盖浏览器默认的新标签页
      "newtab": "index.html"
  },

  "default_locale": "zh_CN",
}